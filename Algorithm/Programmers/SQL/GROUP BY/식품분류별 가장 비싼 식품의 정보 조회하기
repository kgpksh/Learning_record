-- 코드를 입력하세요
SELECT F.CATEGORY, R.MAX_PRICE, PRODUCT_NAME
FROM FOOD_PRODUCT F, (SELECT CATEGORY, MAX(PRICE) MAX_PRICE
                            FROM FOOD_PRODUCT
                           GROUP BY CATEGORY) R
WHERE F.CATEGORY = R.CATEGORY AND F.CATEGORY IN ('과자', '국', '김치', '식용유') AND F.PRICE = R.MAX_PRICE
GROUP BY CATEGORY
ORDER BY F.PRICE DESC